                ],
                "responses": {
                    "200": {
                        "description": "pet response",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "?fun=get-trust&ctxt=SampleWebApp1"
                            }
                        }
                    },
                    "default": {
                        "description": "unexpected error",
