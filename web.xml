<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>GTSimpleEAIServlet</display-name>
  <servlet>
    <servlet-name>GTSimpleEAIServlet</servlet-name>
    <servlet-class>com.ibm.cio.innovation.security.gt.eai.GTSimpleEAIServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>GTSimpleEAIServlet</servlet-name>
    <url-pattern>/GTSimpleEAIServlet</url-pattern>
  </servlet-mapping>
  	<security-constraint>
		<display-name>UserConstraint</display-name>
		<web-resource-collection>
			<web-resource-name>UserCollection</web-resource-name>
			<url-pattern>/*</url-pattern>
		</web-resource-collection>
		
		<auth-constraint>
			<role-name>any-authenticated</role-name>
		</auth-constraint>
		<user-data-constraint>
			<transport-guarantee>CONFIDENTIAL</transport-guarantee>
		</user-data-constraint>
	</security-constraint>
</web-app>
